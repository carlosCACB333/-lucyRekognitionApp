{
  "expo": {
    "name": "recognition",
    "slug": "recognition",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.carloscacb333.recognition"
    },
    "web": {
      "bundler": "metro",
      "favicon": "./assets/images/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "b8729d89-2747-4e71-b6b8-b26ab296ce1b"
      }
    }
  },
  "plugins": [
    "expo-camera",
    {
      "cameraPermission": "Permitir  ${PRODUCT_NAME} para acessar a c√¢mera?"
    },
    "expo-image-picker",
    {
      "photosPermission": "Permitir ${PRODUCT_NAME} para acessar as fotos?"
    }
  ]
}
